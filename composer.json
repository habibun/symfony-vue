{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.8",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "ramsey/uuid-doctrine": "^1.5",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "4.3.*",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/maker-bundle": "^1.14",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "4.3.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/twig-pack": "^1.0",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "4.3.*",
        "thecodingmachine/safe": "^0.1.16"
    },
    "require-dev": {
        "doctrine/coding-standard": "^6.0",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "phpstan/phpstan": "^0.11.19",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/phpunit-bridge": "^4.3",
        "thecodingmachine/phpstan-safe-rule": "^0.1.4",
        "thecodingmachine/phpstan-strict-rules": "^0.11.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "csfix": "phpcbf --ignore=src/Migrations/**,src/Kernel.php src",
        "cscheck": "phpcs --ignore=src/Migrations/**,src/Kernel.php",
        "phpstan": "phpstan analyse src/ -c phpstan.neon --level=7 --no-progress -vvv --memory-limit=1024M",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.3.*"
        }
    }
}
